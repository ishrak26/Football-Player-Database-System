<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.ClubLoginWindowController">
   <children>
      <Label layoutX="160.0" layoutY="135.0" text="username" />
      <Label layoutX="160.0" layoutY="183.0" text="password" />
      <TextField fx:id="usernameTextField" layoutX="244.0" layoutY="131.0" />
      <Button fx:id="loginButton" layoutX="218.0" layoutY="268.0" mnemonicParsing="false" onAction="#login" text="Login" />
      <Button fx:id="resetButton" layoutX="300.0" layoutY="268.0" mnemonicParsing="false" onAction="#reset" text="Reset" />
      <Button fx:id="registerButton" layoutX="367.0" layoutY="268.0" mnemonicParsing="false" onAction="#register" text="Register" />
      <PasswordField fx:id="passwordTextField" layoutX="244.0" layoutY="179.0" />
   </children>
</AnchorPane>
